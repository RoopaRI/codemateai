{
  "name": "codemateai",
  "displayName": "codemateai",
  "description": "VSCode extension to generate Git commands from Natural Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:codemateai.gitCommands" 
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "codemateai.gitCommands",
      "title": "Git Commands"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
